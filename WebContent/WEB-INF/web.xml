<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>Jcrawler</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
        <servlet-name>crawler6PmServelet</servlet-name>
        <servlet-class>com.taohai.Crawler6PmServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>crawler6PmServelet</servlet-name>
        <url-pattern>/6pm</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>amazonServelet</servlet-name>
        <servlet-class>com.taohai.AmazonServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>amazonServelet</servlet-name>
        <url-pattern>/amazon</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>vitacostServelet</servlet-name>
        <servlet-class>com.taohai.VitacostServelet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>vitacostServelet</servlet-name>
        <url-pattern>/vitacost</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>mailServelet</servlet-name>
        <servlet-class>com.taohai.MailServelet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>mailServelet</servlet-name>
        <url-pattern>/receveMail</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>uonecityServelet</servlet-name>
        <servlet-class>com.taohai.UOneCityServelet</servlet-class>
    </servlet>
     <servlet-mapping>
        <servlet-name>uonecityServelet</servlet-name>
        <url-pattern>/order</url-pattern>
    </servlet-mapping>
</web-app>
